<> mod(a,b) --> a % b

fn mod(int a,int b) => int
{

    if(b == 0) 
    {
        ret a;
    }
    else{
        ret a - a/b;
    }

}

fn isPrime(int x) => int
{
    int i;
    int res = 1;
    int div;

    for(i = 2;i<x; i++)
    {
        <> if its not prime set the result to false
        if(mod(x,i) == 0) res = 0;
    }

    ret res;
}

